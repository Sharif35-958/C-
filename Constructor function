#include <iostream>
using namespace std;

class student{
  public:
    int id;
    double gpa;
    
  void display(){
    cout<<id<<" "<<gpa<<endl;
  }
  
  student(int x,double y){
    id=x;
    gpa=y;
  }
};

int main() 
{
 student Alim(101,3.78);
 Alim.display();
 
 student Moga(102,2.50);
 Moga.display();
}
